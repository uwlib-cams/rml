    <rdau:P60001>Beavers -- Ecology -- North America.</rdau:P60001>
    <rdae:P20214 xml:lang="eng">2018</rdae:P20214>
    <rdae:P20267 xml:lang="eng">map of North American beaver range</rdae:P20267>
    <sin:hasResourceTemplate>WAU:RT:RDA:Expression:monograph</sin:hasResourceTemplate>

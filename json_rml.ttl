@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix ex: <http://example.org/rules/> .

ex:URIMap a rr:TriplesMap;
  rml:logicalSource [
    rml:source "/home/mcm104/http-sinopia-mongo/uw_list.json";
    rml:referenceFormulation ql:JSONPath;
    rml:iterator "$.data[*]"
  ].

ex:URIMap rr:subjectMap [
  rr:constant "ex:UW_URIs"
].

ex:URIMap rr:predicateObjectMap [
	rr:predicate "ex:hasURI";
	rr:objectMap [
		rml:reference "uri";
		rr:termType rr:IRI
	]
].

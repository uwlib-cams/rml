# summary >> Summary > rdfs:label
ex:SummaryMap a rr:TriplesMap ;
	rml:logicalSource [
		rml:source "../expression.xml" ;
		rml:referenceFormulation ql:XPath ;
		rml:iterator "rdf:RDF/rdf:Description[rdae:P20069[not(@rdf:resource)]]"
	] ;


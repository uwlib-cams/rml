		rr:termType rr:BlankNode ;
		rr:class bf:Summary
	] ;

	rr:predicateObjectMap [
		rr:predicate rdfs:label ;
		rr:objectMap [
			rml:reference "rdae:P20069[not(@rdf:resource)]" ;

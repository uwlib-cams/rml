# instanceOf >> Work > rdfs:label
ex:InstanceOfMap a rr:TriplesMap ;
	rml:logicalSource [
		rml:source "../manifestation.xml" ;
		rml:referenceFormulation ql:XPath ;
		rml:iterator "rdf:RDF/rdf:Description[rdam:P30139[not(@rdf:resource)]]"
	] ;

	rr:subjectMap [

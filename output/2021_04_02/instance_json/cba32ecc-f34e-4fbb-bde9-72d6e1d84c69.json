tus": [{"@id": "_:N94135f30046a412884364155839e7221"}]}, {"@id": "_:Nc24b7f6b5d644de2b7ef0e85475b385e", "@type": ["http://id.loc.gov/ontologies/bibframe/GenerationProcess"], "http://www.w3.org/2000/01/rdf-schema#label": [{"@type": "http://www.w3.org/2001/XMLSchema#dateTime", "@value": "rml.py SNAPSHOT: 2021-04-02T13:02:28+00:00"}]}, {"@id": "_:N94135f30046a412884364155839e7221", "@type": ["http://id.loc.gov/ontologies/bibframe/Status"], "http://id.loc.gov/ontologies/bibframe/code": [{"@value": "n"}]}, {"@id": "_:N26a5fad9eb3d43efbad8758032730e5c", "@type": ["http://id.loc.gov/ontologies/bibframe/Title"], "http://id.loc.gov/ontologies/bibframe/mainTitle": [{"@language": "eng", "@value": "The house of the dead ; and, Poor folk"}]}, {"@id": "_:N03fa4e418ee64b76b200e6b3b625c5f9", "@type": ["http://id.loc.gov/ontologies/bibframe/Publication"], "http://id.loc.gov/ontologies/bibframe/agent": [{"@id": "_:N1d11ef5291434454bbb44696980516c5"}], "http://id.loc.gov/ontologies/bibframe/date": [{"@type": "http://www.w3.org/2001/XMLSchema#gYear", "@value": "2004-01-01"}], "http://id.loc.gov/ontologies/bibframe/place": [{"@id": "_:N25dfce676bce4029881b7c3c7920ba25"}]}, {"@id": "_:N1d11ef5291434454bbb44696980516c5", "@type": ["http://id.loc.gov/ontologies/bibframe/Agent"], "http://www.w3.org/2000/01/rdf-schema#label": [{"@language": "eng", "@value": "Barnes & Noble Classics"}]}, {"@id": "_:N25dfce676bce4029881b7c3c7920ba25", "@type": ["http://id.loc.gov/ontologies/bibframe/Place"], "http://www.w3.org/2000/01/rdf-schema#label": [{"@language": "eng", "@value": "New York, NY"}]}, {"@id": "_:N6b4442dce7f64add9bf9d72e56ce6719", "@type": ["http://id.loc.gov/ontologies/bibframe/Note"], "http://www.w3.org/2000/01/rdf-schema#label": [{"@language": "eng", "@value": "\"Arrested in 1849 for belonging to a secret group of radical utopians, Fyodor Dostoevsky was sentenced to four years in a Siberian labor camp--a terrible mental, spiritual, and physical ordeal that inspired him to write the novel 'The house of the dead'. Told from the point of view of a fictitious narrator--a convict serving a ten-year sentence for murdering his wife--'The house of the dead' describes in vivid detail the horrors that Dostoevsky himself witnessed while in prison: the brutality of guards who relish cruelty for its own sake; the evil of criminals who enjoy murdering children; and the existence of decent souls amid filth and degradation. Also included in this volume is Dostoevsky's first published work, 'Poor folk', a novel written in the form of letters that brought him immediate critical and public recognition\"--Page 4 of cover."}]}], "user": "mcm104", "group": "washington", "templateId": "WAU:RT:BF2:Instance", "types": ["http://id.loc.gov/ontologies/bibframe/Instance"], "id": "cba32ecc-f34e-4fbb-bde9-72d6e1d84c69", "uri": "https://api.stage.sinopa.io/resource/cba32ecc-f34e-4fbb-bde9-72d6e1d84c69", "timestamp": "2021-04-02T13:31:28"}